<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.cms.launcher.dao.SponsorMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.cms.launcher.entity.SponsorForValidated">
        <id column="sponsor_id" property="sponsorId" />
        <result column="sponsor_name" property="sponsorName" />
        <result column="sponsor_statement" property="sponsorStatement" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        sponsor_id, sponsor_name, sponsor_statement
    </sql>

</mapper>
